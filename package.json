{
  "name": "prompt-or-die",
  "version": "1.0.0",
  "type": "module",
  "packageManager": "bun@1.0.0",
  "main": "packages/cli/dist/interactive.js",
  "bin": {
    "prompt-or-die": "packages/cli/dist/interactive.js"
  },
  "scripts": {
    "start": "npm run build:cli && node packages/cli/dist/interactive.js",
    "build": "npm run build:core && npm run build:cli && npm run build:web",
    "build:cli": "cd packages/cli && npm run build",
    "build:core": "cd packages/core && npm run build",
    "build:web": "npm run copy:packages && vite build",
    "copy:packages": "npm run copy:core",
    "copy:core": "node -e \"const fs=require('fs');const path=require('path');const src='packages/core/dist';const dest='node_modules/prompt-or-die-core';fs.mkdirSync(dest,{recursive:true});fs.cpSync(src,dest,{recursive:true});fs.copyFileSync('packages/core/package.json',path.join(dest,'package.json'));\"",
    "dev": "vite",
    "dev:cli": "cd packages/cli && npm run dev",
    "build:dev": "vite build --mode development",
    "build:sdk": "npm run build:core",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@inquirer/password": "^4.0.15",
    "@radix-ui/react-accordion": "^1.2.0",
